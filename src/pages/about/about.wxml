<view>
  <!-- 页面头部 -->
  <view class="about-header">
    <image class="logo" src="/images/cnodejs.svg"></image>
    <view class="fw-300">cnodejs.org</view>
  </view>
  <!-- 页面主体内容 -->
  <view class="about-content-list">
    <view class="content-item" wx:for="{{aContentList}}" wx:key="{{index}}">
      <view class="item-header">{{item.header}}</view>
      <view class="item-content">{{item.content}}</view>
    </view>
  </view>
</view>